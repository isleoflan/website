---
// Component Imports
import MainHeader from '../components/MainHead.astro';
import Header from '../layouts/Header.astro';
import NextEventHeader from '../layouts/NextEventHeader.astro';

import CardContent from '../layouts/CardContent.astro';

import Faq from '../content/cards/Faq.astro';
import Footer from '../layouts/Footer.astro';

const title = `Frequently asked questions`;
const description = ``;
const keywords = ['Frequently asked questions', 'Isle of LAN', 'Sulgen'];
const type = 'website';
const canonicalURL = Astro.href;
---
<html lang="de">
<head>
   <MainHeader
      title={title}
      description={description}
      keywords={keywords.join(', ')}
      image=""
      type={type}
      canonicalURL={canonicalURL}
   />
</head>
<body>
   <Header />
   <CardContent>
      <Faq />
   </CardContent>
   <Footer />
</body>
</html>
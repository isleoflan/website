<style lang="scss">
  @use "../style/abstracts" as *;

  .seatplan {
    padding-top: 5em;
    padding-bottom: 5em;
    float: left;
    width: 100%;
    &__svg {
      margin-top: 2em;
      width: 100%;
      @include for-desktop-up {
        width: 80%;
      }

      position: relative;

      svg {
        width: 100%;
      }
    }

    &__claninfo {
      background: linear-gradient(45deg,#4a248c 50%,#af1aac 100%);
      font-weight: bold;
      color: #FFFFFF;
      width: 60%;
      padding: 10px;
      text-align: center;
      margin: 40px 0;

      .seatplan__claninfo--text{
        width: 100%;
        padding: 20px 80px;
        background: #FFFFFF;
        color: #000000;

        .text--small {
          font-size: .7rem;
          margin-top: 1rem;
        }
      }
    }
  }

  .seat {
    &__status {
      &--ready,
      &--reserved {
        fill: $yellow-dark;
      }
      &--free {
        fill: rgba(0, 0, 0, 0.1);
      }
    }

    &__popup {
      position: absolute;
      display: flex;
      flex-direction: row;
      align-items: center;
      background-color: $yellow-dark;
      font-size: 0.8rem;
      height: 24px;
      transform: translateX(-50%);

      img {
        height: 24px;
      }
      span {
        padding: 0 5px;
        background-color: $yellow-dark;
        position: relative;
        z-index: 1;
        strong {
          &::before {
            content: "[";
          }
          &::after {
            content: "]";
          }
        }
      }

      &::after {
        content: "";
        display: block;
        height: 12px;
        width: 12px;
        transform: translateX(-50%) rotate(45deg);
        transform-origin: center center;
        position: absolute;
        top: -5px;
        left: 50%;
        background-color: $yellow-dark;
      }
    }
  }
</style>
